<div class="row">
  <div class="card">
    <div class="card-body">
      <h6 class="card-subtitle mb-2 text-muted"><span
          id="log-emoji"
        >🚴‍♀️</span>{{singleLog.workout_type}}</h6>
      <p class="card-text">{{singleLog.details}}
        id:
        {{log.id}}</p>

      <a
        data-bs-toggle="tooltip"
        data-bs-placement="bottom"
        data-bs-title="delete this log"
        data-id={{singleLog.id}}
      >
        <i class="bi bi-three-dots-vertical" style="color:pink;"></i></a>
    </div>
  </div>
</div>